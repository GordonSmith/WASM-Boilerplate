{
  "name": "wasm-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.es6",
  "unpkg": "dist/index.min.js",
  "types": "types/index.d.ts",
  "scripts": {
    "preinstall": "wsl -u root -e ./cpp-preinstall.sh",
    "postinstall": "wsl -e ./cpp-postinstall.sh",
    "clean": "rimraf build lib-*",
    "build-cpp": "wsl -e ./cpp-build.sh",
    "build-ts": "tsc",
    "build-ts-es6": "tsc --module es6 --outDir ./lib-es6",
    "build": "run-s build-cpp build-ts build-ts-es6",
    "bundle": "cpx ./build/cpp/main/MyLib.wasm ./dist && npm run build-ts-es6 && rollup -c",
    "serve": "ws --open",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "tslib": "1.9.3"
  },
  "devDependencies": {
    "cpx": "1.5.0",
    "local-web-server": "2.6.1",
    "npm-run-all": "4.1.5",
    "rimraf": "2.6.3",
    "rollup": "1.12.4",
    "rollup-plugin-alias": "1.5.1",
    "rollup-plugin-commonjs": "10.0.0",
    "rollup-plugin-node-resolve": "5.0.0",
    "rollup-plugin-postcss": "2.0.3",
    "terser": "3.17.0",
    "tslint": "5.16.0",
    "typescript": "3.4.5"
  }
}
